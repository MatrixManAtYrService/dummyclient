  "components": {
    "schemas": {
      "ActionType": {
        "type": "string",
        "enum": [
          "add",
          "subtract"
        ],
        "title": "ActionType",
        "description": "Valid operation actions."
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "NumberOperation": {
        "properties": {
          "action": {
            "$ref": "#/components/schemas/ActionType"
          },
          "value": {
            "type": "integer",
            "title": "Value"
          }
        },
        "type": "object",
        "required": [
          "action",
          "value"
        ],
        "title": "NumberOperation",
        "description": "Request model for number operations."
      },
      "NumberResponse": {
        "properties": {
          "number": {
            "type": "integer",
            "title": "Number"
          }
        },
        "type": "object",
        "required": [
          "number"
        ],
        "title": "NumberResponse",
        "description": "Response model for number operations."
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
